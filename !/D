btnDt:addEventListener("tap", function()
    cloudOn.show({
        time = 300
    })
    timer.performWithDelay(300, function()
        composer.removeScene("interfaces.btnKaguya")
        composer.gotoScene("interfaces.btnKaguya")
    end)
    timer.performWithDelay(300, function()
        cloudOff.show({
            group = display.getCurrentStage(),
            time = 600
        })
    end)
end)
